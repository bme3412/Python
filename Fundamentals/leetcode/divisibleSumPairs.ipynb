   "outputs": [],
   "source": [
    "def divisibleSumPairs(n, k, ar):\n",
    "    count = 0\n",
    "    for i in range(n):\n",
    "        for j in range(i+1,n):\n",
    "            if (ar[i] + ar[j]) % k ==0:\n",
    "                count+=1\n",
    "    return count\n"
   ]
